{
  "name": "react-pull-stream",
  "version": "2.0.2",
  "main": "index.js",
  "scripts": {
    "deps": "dependency-check package.json && dependency-check package.json --unused --entry test/*.js -i dependency-check -i eslint -i eslint-config-invintus -i tap-spec -i browserify -i wzrd -i tape-run",
    "lint": "eslint .",
    "preversion": "npm run lint && npm run deps && npm test",
    "postversion": "git push && git push --tags && npm publish",
    "dev": "wzrd example/index.js -- -dv",
    "test": "browserify test/index.js | tape-run | tap-spec"
  },
  "dependencies": {
    "create-react-class": "^15.6.2",
    "pull-abortable": "^4.1.0",
    "pull-notify": "^0.1.1",
    "pull-pushable": "^2.0.1",
    "pull-stream": "^3.4.5",
    "xtend": "^4.0.1"
  },
  "peerDependencies": {
    "react": "^15.3.2"
  },
  "devDependencies": {
    "browserify": "^13.1.1",
    "dependency-check": "^2.8.0",
    "eslint": "^4.8.0",
    "eslint-config-invintus": "^8.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "react-dom": "^15.3.2",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.2",
    "tape-run": "^2.1.4",
    "wzrd": "^1.4.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tvwit/react-pull-stream.git"
  },
