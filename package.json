  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tvwit/react-pull-stream/issues"
  },
  "homepage": "https://github.com/tvwit/react-pull-stream#readme",
  "description": "Create a duplex stream from a react component.",
  "directories": {
    "example": "example",
    "test": "test"
  },
  "keywords": [
    "react",
    "pull",
    "stream",
    "pull-stream",
    "duplex"
  ]
